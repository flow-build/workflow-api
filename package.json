  "scripts": {
    "test": "jest --watch --coverage --runInBand --forceExit",
    "test:single": "jest -i --detectOpenHandles --forceExit",
    "tests": "npm -- test",
    "seeds": "knex --env ${KNEX_ENV} --knexfile knexfile.js seed:run",
    "seeds:local": "knex --env dockerLocal --knexfile knexfile.js seed:run",
    "migrations": "knex --env ${KNEX_ENV} --knexfile knexfile.js migrate:latest",
    "migrations:local": "knex --env dockerLocal --knexfile knexfile.js migrate:latest",
    "start": "nodemon src/server.js",
    "watch": "nodemon --inspect=0.0.0.0 src/server.js",
    "get-version": "echo $npm_package_version",
    "release": "semantic-release",
    "export": "node ./scripts/export.js",
    "lint": "eslint ./src  --ext .js --max-warnings=0"
  },
  "devDependencies": {
